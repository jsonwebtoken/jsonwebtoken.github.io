  body(data-spy='scroll', data-target='.navbar-default')
    // Fixed navbar
    nav.navbar.navbar-default.navbar-fixed-top(role='navigation')
      .container
        // Brand and toggle get grouped for better mobile display
        .navbar-header
          button.navbar-toggle.collapsed(type='button', data-toggle='collapse', data-target='#bs-example-navbar-collapse-1')
            span.sr-only Toggle navigation
            span.icon-bar
            span.icon-bar
            span.icon-bar
          a.navbar-brand(href='#')
            img.hid-m(src='img/pic_logo.svg', alt='')
            img(src='img/logo.svg', alt='')
        // Collect the nav links, forms, and other content for toggling
        #bs-example-navbar-collapse-1.collapse.navbar-collapse
          ul.nav.navbar-nav
            li.active
              a(href='#debugger') Debugger
            li
              a(href='#libraries') Libraries
            li
              a(href='#') Support
            li
              a(href='https://ask.auth0.com/category/jwt', target='_blank') Ask
          a.design(href='#')
            | Supported by
            img(src='img/ico_logo.svg', alt='')
          a.twitter.mobile-no(href='#')
            img(src='img/ico_twitter.svg', alt='')

    section.banner-jwt
      //- canvas
      .container
        a.logo(href='#')
          img(src='img/pic_logo.svg', alt='')
        p
          strong JSON Web Tokens
          |  are an open, industry standard
          span  [RFC 7519]
          |  method for representing claims securely between two parties.
          strong  JWT.IO
          |  allows you to decode, verify and generate JWT.
        a.btn.btn-purple.btn-lg(href='#') LEARN MORE

    .container
      .page-header
        a(name='debugger')
        h1#debugger Debugger
      .jwt-playground
        .row
          .col-md-12
            .algorithm
              label.algorithm-label Algorithm:
              .hide
                label.radio-inline
                  input(type='radio', name='algorithm', value='HS256', checked='checked')
                  |  HS256
                label.radio-inline
                  input(type='radio', name='algorithm', value='RS256')
                  |  RS256
              select#selectAl.selectpicker
                option(selected='', value='HS256') HS256
                option(value='RS256') RS256
          .tabs
            .tab-nav
              ul
                li.tab-link.current
                  a(href='#tab-1')
                    | Encoded
                    small.pull-right paste a token here
                li.tab-link
                  a(href='#tab-2')
                    | Decoded
                    small.pull-right edit the payload and secret (only HS256 supported)
            #tab-1.input.js-input.tab-content(style='display: block;')

            #tab-2.output.tab-content
              .jwt-explained.jwt-header
                p.text-line
                  span HEADER:
                  |  ALGORITHM & TOKEN TYPE
                  a(href='#') LEARN MORE
                .js-header

              .jwt-explained.jwt-payload
                p.text-line
                  span PAYLOAD:
                  |  DATA
                  a(href='#') LEARN MORE
                .js-payload

              .jwt-explained.jwt-signature
                p.text-line
                  span VERIFY SIGNATURE:
                  |   LOREM IPSUM DOLOR SIT
                  a(href='#') LEARN MORE
                pre.HS256.
                  HMACSHA256(
                    base64UrlEncode(header) + "." +
                    base64UrlEncode(payload),
                    <input type="text" name="secret" value="secret">
                  )<span class="is-base64-encoded-label"> <input type="checkbox" name="is-base64-encoded">secret base64 encoded</span>
                pre.RS256.
                  RSASHA256(
                    base64UrlEncode(header) + "." +
                    base64UrlEncode(payload),
                    <textarea rows="4" name="public-key" placeholder="Public Key or Certificate. Enter it in plain text only if you want to verify a token"></textarea>,
                    <textarea rows="5" name="private-key" placeholder="Private Key (RSA). Enter the it in plain text only if you want to generate a new token. The key never leaves your browser."></textarea>
                  )
        .row
          .col-md-12
            .validation-status.js-signature

